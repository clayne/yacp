  /usr/share/doc/rtfilter/README.md

(librtfilter1)
  /usr/bin/cygrtfilter-1.dll

(librtfilter-devel)
  /usr/bin/librtfilter-2.def
  /usr/include/rtf_common.h
  /usr/include/rtfilter.h
  /usr/lib/librtfilter.dll.a
  /usr/lib/pkgconfig/rtfilter.pc
  /usr/share/doc/rtfilter/doc/examples/Makefile
  /usr/share/doc/rtfilter/doc/examples/butterworth.c

