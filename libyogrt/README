  /usr/share/man/man3/yogrt_get_interval2_start.3.gz
  /usr/share/man/man3/yogrt_remaining.3.gz
  /usr/share/man/man3/yogrt_set_debug.3.gz
  /usr/share/man/man3/yogrt_set_fudge_factor.3.gz
  /usr/share/man/man3/yogrt_set_interval1.3.gz
  /usr/share/man/man3/yogrt_set_interval2.3.gz
  /usr/share/man/man3/yogrt_set_interval2_start.3.gz
  /usr/share/man/man3/yogrt_set_remaining.3.gz
  /usr/share/man/man5/yogrt.conf.5.gz
  /usr/share/man/man7/libyogrt.7.gz
  /usr/share/man/man7/yogrt.7.gz

