  /usr/lib/python3.8/site-packages/proto/__pycache__/_utils.cpython-38.opt-2.pyc
  /usr/lib/python3.8/site-packages/proto/__pycache__/_utils.cpython-38.pyc
  /usr/lib/python3.8/site-packages/proto/__pycache__/nanopb_pb2.cpython-38.opt-1.pyc
  /usr/lib/python3.8/site-packages/proto/__pycache__/nanopb_pb2.cpython-38.opt-2.pyc
