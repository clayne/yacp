  /usr/share/doc/rgbds/html/mandoc.css
  /usr/share/doc/rgbds/html/rgbasm.1.html
  /usr/share/doc/rgbds/html/rgbasm.5.html
  /usr/share/doc/rgbds/html/rgbds.5.html
  /usr/share/doc/rgbds/html/rgbds.7.html
  /usr/share/doc/rgbds/html/rgbds.css
  /usr/share/doc/rgbds/html/rgbfix.1.html
  /usr/share/doc/rgbds/html/rgbgfx.1.html
  /usr/share/doc/rgbds/html/rgblink.1.html
  /usr/share/doc/rgbds/html/rgblink.5.html

