  /usr/lib/libhiredis.dll.a
  /usr/lib/pkgconfig/hiredis.pc

(libhiredis_ssl1.0.1)
  /usr/bin/cyghiredis_ssl-1.0.1.dll

(libhiredis_ssl-devel)
  /usr/include/hiredis/hiredis_ssl.h
  /usr/lib/cmake/hiredis_ssl/hiredis_ssl-config.cmake
