  /usr/include/libbfoverlay/codepage.h
  /usr/include/libbfoverlay/definitions.h
  /usr/include/libbfoverlay/error.h
  /usr/include/libbfoverlay/extern.h
  /usr/include/libbfoverlay/features.h
  /usr/include/libbfoverlay/types.h
  /usr/lib/libbfoverlay.dll.a
  /usr/lib/pkgconfig/libbfoverlay.pc
  /usr/share/man/man3/libbfoverlay.3.gz

