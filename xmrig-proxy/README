  /usr/share/doc/xmrig-proxy/STRATUM.md
  /usr/share/doc/xmrig-proxy/STRATUM_EXT.md
  /usr/share/doc/xmrig-proxy/gpg_keys/xmrig.asc
  /usr/share/doc/xmrig-proxy/releases/5_0_0/SHA256SUMS
  /usr/share/doc/xmrig-proxy/releases/5_0_0/SHA256SUMS.sig
  /usr/share/xmrig-proxy/config.json

