  /usr/share/doc/tinysvm/html/svm_classify.html
  /usr/share/doc/tinysvm/html/svm_learn.html
  /usr/share/doc/tinysvm/html/svm_model.html
  /usr/share/doc/tinysvm/html/tinysvm.css
  /usr/share/man/man1/svm_classify.1.gz
  /usr/share/man/man1/svm_learn.1.gz
  /usr/share/man/man1/svm_model.1.gz

(libtinysvm1)
  /usr/bin/cygtinysvm-1.dll

(libtinysvm-devel)
