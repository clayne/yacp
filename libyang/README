  /usr/include/libyang/extensions.h
  /usr/include/libyang/libyang.h
  /usr/include/libyang/swigpyrun.h
  /usr/include/libyang/tree_data.h
  /usr/include/libyang/tree_schema.h
  /usr/include/libyang/user_types.h
  /usr/include/libyang/xml.h
  /usr/lib/libyang-cpp.dll.a
  /usr/lib/libyang.dll.a
  /usr/lib/pkgconfig/libyang-cpp.pc
  /usr/lib/pkgconfig/libyang.pc

(python38-libyang)
  /usr/lib/python3.8/site-packages/__pycache__/yang.cpython-38.opt-1.pyc
