  /usr/share/doc/stoken/sdtid-test.pl
  /usr/share/man/man1/stoken-gui.1.gz
  /usr/share/man/man1/stoken.1.gz
  /usr/share/pixmaps/stoken-gui.png
  /usr/share/stoken/password-dialog.ui
  /usr/share/stoken/pin-dialog.ui
  /usr/share/stoken/tokencode-detail.ui
  /usr/share/stoken/tokencode-small.ui

(libstoken1)
  /usr/bin/cygstoken-1.dll

(libstoken-devel)
