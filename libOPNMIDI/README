  /usr/share/doc/libOPNMIDI/wopn/gs-by-papiezak-and-sneakernets-readme.txt
  /usr/share/doc/libOPNMIDI/wopn/readme.txt
  /usr/share/sounds/wopn/Tomsoft.wopn
