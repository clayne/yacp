  /usr/include/bgpstream_elem.h
  /usr/include/bgpstream_record.h
  /usr/include/bgpstream_utils.h
  /usr/include/bgpstream_utils_addr.h
  /usr/include/bgpstream_utils_addr_set.h
  /usr/include/bgpstream_utils_as_path.h
  /usr/include/bgpstream_utils_as_path_store.h
  /usr/include/bgpstream_utils_community.h
  /usr/include/bgpstream_utils_id_set.h
  /usr/include/bgpstream_utils_ip_counter.h
  /usr/include/bgpstream_utils_patricia.h
  /usr/include/bgpstream_utils_peer_sig_map.h
  /usr/include/bgpstream_utils_pfx.h
  /usr/include/bgpstream_utils_pfx_set.h
  /usr/include/bgpstream_utils_str_set.h
  /usr/include/bgpstream_utils_time.h
  /usr/lib/libbgpstream.dll.a
  /usr/lib/pkgconfig/libbgpstream.pc

