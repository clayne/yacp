  /usr/share/doc/zlibcomplete/LICENSE
  /usr/share/doc/zlibcomplete/NEWS
  /usr/share/doc/zlibcomplete/README
  /usr/share/doc/zlibcomplete/README.md

(libzlibcomplete0)
  /usr/bin/cygzlibcomplete-0.dll

(libzlibcomplete-devel)
  /usr/include/zlc/gzipcomplete.hpp
  /usr/include/zlc/zlibbase.hpp
  /usr/include/zlc/zlibcomplete.hpp
  /usr/include/zlc/zlibmisc.hpp
  /usr/include/zlc/zlibraw.hpp
  /usr/lib/libzlibcomplete.dll.a
  /usr/lib/pkgconfig/zlibcomplete.pc

