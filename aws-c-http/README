  /usr/include/aws/http/exports.h
  /usr/include/aws/http/http.h
  /usr/include/aws/http/http2_stream_manager.h
  /usr/include/aws/http/proxy.h
  /usr/include/aws/http/request_response.h
  /usr/include/aws/http/server.h
  /usr/include/aws/http/statistics.h
  /usr/include/aws/http/status_code.h
