(libspkg0)
  /usr/bin/cygspkg-0.dll

(libspkg-devel)
  /usr/include/libspkg/commands.h
  /usr/include/libspkg/error.h
  /usr/include/libspkg/message.h
  /usr/include/libspkg/misc.h
