  /usr/share/doc/protobuf-c/CONTRIBUTING.md
  /usr/share/doc/protobuf-c/ChangeLog.old
  /usr/share/doc/protobuf-c/LICENSE
  /usr/share/doc/protobuf-c/README.md
  /usr/share/doc/protobuf-c/TODO

(libprotobuf-c1)
  /usr/bin/cygprotobuf-c-1.dll

(libprotobuf-c-devel)
  /usr/include/google/protobuf-c/protobuf-c.h
  /usr/include/protobuf-c/protobuf-c.h
  /usr/include/protobuf-c/protobuf-c.proto
  /usr/lib/libprotobuf-c.dll.a
  /usr/lib/pkgconfig/libprotobuf-c.pc

