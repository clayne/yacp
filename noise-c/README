  /usr/include/noise/protocol/cipherstate.h
  /usr/include/noise/protocol/constants.h
  /usr/include/noise/protocol/dhstate.h
  /usr/include/noise/protocol/errors.h
  /usr/include/noise/protocol/handshakestate.h
  /usr/include/noise/protocol/hashstate.h
  /usr/include/noise/protocol/names.h
  /usr/include/noise/protocol/randstate.h
  /usr/include/noise/protocol/signstate.h
  /usr/include/noise/protocol/symmetricstate.h
  /usr/include/noise/protocol/util.h
  /usr/lib/libnoisekeys.dll.a
  /usr/lib/libnoiseprotobufs.dll.a
  /usr/lib/libnoiseprotocol.dll.a

