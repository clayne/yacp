  /usr/share/doc/libClustalW/README

(libClustalW1.0_1)
  /usr/bin/cygClustalW-1.0-1.dll

(libClustalW1.0-devel)
  /usr/include/libClustalW-1.0/libClustalW/clustalw.h
  /usr/include/libClustalW-1.0/libClustalW/dayhoff.h
  /usr/include/libClustalW-1.0/libClustalW/general.h
  /usr/include/libClustalW-1.0/libClustalW/matrices.h
  /usr/include/libClustalW-1.0/libClustalW/param.h
  /usr/lib/libClustalW-1.0.dll.a
