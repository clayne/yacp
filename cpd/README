  /usr/lib/cmake/cpd/cpd-config.cmake
  /usr/lib/cmake/cpd/cpd-targets-release.cmake
  /usr/lib/cmake/cpd/cpd-targets.cmake
  /usr/lib/libcpd.dll.a
