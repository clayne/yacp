  /usr/bin/centrifuge-class.exe
  /usr/bin/centrifuge-compress.pl
  /usr/bin/centrifuge-download
  /usr/bin/centrifuge-inspect
  /usr/bin/centrifuge-inspect-bin.exe
  /usr/bin/centrifuge-kreport
  /usr/bin/centrifuge-promote
  /usr/bin/centrifuge-sort-nt.pl
  /usr/share/centrifuge/indices/Makefile
  /usr/share/doc/Cygwin/centrifuge.README
  /usr/share/doc/centrifuge/AUTHORS
  /usr/share/doc/centrifuge/LICENSE
  /usr/share/doc/centrifuge/MANUAL
  /usr/share/doc/centrifuge/MANUAL.markdown
  /usr/share/doc/centrifuge/NEWS
