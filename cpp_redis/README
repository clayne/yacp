  /usr/include/cpp_redis/core/client.hpp
  /usr/include/cpp_redis/core/reply.hpp
  /usr/include/cpp_redis/core/sentinel.hpp
  /usr/include/cpp_redis/core/subscriber.hpp
  /usr/include/cpp_redis/cpp_redis
