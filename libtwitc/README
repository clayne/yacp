  /usr/share/doc/libtwitc/README.md
  /usr/share/doc/libtwitc/TODO

(libtwitc0)
  /usr/bin/cygtwitc-0.dll

(libtwitc-devel)
  /usr/include/libtwitc/dm.h
  /usr/include/libtwitc/favorites.h
  /usr/include/libtwitc/functions.h
  /usr/include/libtwitc/http.h
  /usr/include/libtwitc/oauth.h
  /usr/include/libtwitc/status.h
  /usr/include/libtwitc/stdredef.h
  /usr/include/libtwitc/timeline.h
  /usr/include/libtwitc/twitc.h
  /usr/include/libtwitc/twitcver.h
  /usr/include/libtwitc/twitter.h
  /usr/include/libtwitc/user.h
  /usr/lib/libtwitc.dll.a
  /usr/lib/pkgconfig/libtwitc.pc

