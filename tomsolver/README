  /usr/include/tomsolver/error_type.h
  /usr/include/tomsolver/functions.h
  /usr/include/tomsolver/linear.h
  /usr/include/tomsolver/mat.h
  /usr/include/tomsolver/math_operator.h
  /usr/include/tomsolver/node.h
  /usr/include/tomsolver/nonlinear.h
  /usr/include/tomsolver/parse.h
  /usr/include/tomsolver/simplify.h
  /usr/include/tomsolver/subs.h
  /usr/include/tomsolver/symmat.h
  /usr/include/tomsolver/tomsolver.h
  /usr/include/tomsolver/vars_table.h
  /usr/lib/cmake/tomsolver/tomsolver-config.cmake
  /usr/lib/cmake/tomsolver/tomsolver-targets-release.cmake
  /usr/lib/cmake/tomsolver/tomsolver-targets.cmake
  /usr/lib/libtomsolver.dll.a
  /usr/lib/pkgconfig/tomsolver.pc

